  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.6",
    "@types/react-test-renderer": "^16.8.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^3.2.0",
    "react-redux": "^7.0.3",
    "react-scripts-ts": "^3.1.0",
    "react-test-renderer": "^16.8.6",
    "react-wooden-tree": "file:./",
    "redux": "^4.0.1",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
