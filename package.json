{
  "name": "react-wooden-tree",
  "version": "2.0.9",
  "private": false,
  "description": "Tree view component for react.",
  "main": "./dist/react-wooden-tree.js",
  "types": "./dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brumik/react-wooden-tree.git"
  },
  "keywords": [
    "tree",
    "view",
    "react",
    "component"
  ],
  "author": "Levente Berky",
  "license": "MIT",
  "bugs": {
